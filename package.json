    "compiler"
  ],
  "author": "Veritas Raven",
  "license": "MIT",
  "homepage": "https://github.com/voorjaar/windicss",
  "dependencies": {
    "arg": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.14",
    "typescript": "^3.7.5"
  },
  "files": [
    "src/**/*.js",
    "src/**/*.d.ts"
  ]
}
